*,body{
	margin:0px auto;
	font-family: Tahoma,"宋体";
}
.find_nav{
	height:83px;
	background:rgb(227,239,251);
}
.find_nav1{
	width:930px;
	height:83px;
}
.find_logo{
	width:385px;
	height:83px;float:left;
}
.find_info{
	margin-top:10px;
	float:right; 
	width:300px;
	height:30px;
}
.find_info ul li{
	width:50px;
	height:20px;
	list-style:none;
	float:left;
    	font-size: 12px;
    	text-align:center;
}
/*导航*/
.find_navigation{
	height:40px;
	border:1px solid rgb(67,115,151);
	background:rgb(57,136,202);
}
.find_nav_content{
	width:950px;
	height:40px;

}
.find_nav_content ul{
	width:550px;
	height:40px;
	float:left;
	padding:0px;
}
.find_nav_content ul li{
	list-style:none;
	width:100px;
	height:40px;
	float:left;
	line-height:40px;
	margin-left:10px;
	text-align:center;
	color:#ffffff;
	font-size:16px;
}
.find_color{
	background:rgb(21,106,170);
}
/*主体部分*/
.find_main{
	background:rgb(223,241,255);
	height:700px;
}
.find_main_content{
	width:930px;
	height:600px;
	border:5px solid rgb(236,239,241);
	border-radius:5px 5px 5px 5px;
	background:#ffffff;
	margin-top:10px;
}
.find_main_manage{
	height:40px;
	border-bottom:1px dashed #ccc;
}
.find_main_manage ul{
	padding:0px;
}
.find_main_manage ul li{
	list-style:none;
	float:left;
	height:40px;
	line-height:40px;
	width:100px;
	text-align:center;
	font-size:14px;
}
.find_form{
	width:465px;
	height:300px;
	margin-top:10px;
}
.find_name{
	width:465px;
	height:50px;
	margin-top:50px;
}
.find_name1{
	width:50px;
	height:30px;
	float:left;
	line-height:30px;
}
.find_name2{
	width:288px;
	height:30px;
	float:left;
}
.find_name2 input{
	height:30px;
	width:288px;
	border:1px solid rgb(49,151,181);
}
.find_email{
	width:465px;
	height:50px;
	margin-top:50px;
}
.find_email1{
	width:50px;
	height:30px;
	float:left;
	line-height:30px;
}
.find_email2{
	width:288px;
	height:30px;
	float:left;
}
.find_email2 input{
	height:30px;
	width:288px;
	border:1px solid rgb(49,151,181);
}
.find_sub{
	height:32px;
	width:200px;
	margin-top:20px;
}
