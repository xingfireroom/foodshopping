.center_main{
	width:990px;
	height:700px;
	margin:0px auto;

    }
.breadnav{
	height:45px;
	line-height:45px;
}
/*左边部分*/
.center_left{
	float:left;
	width:143px;
	height:635px;
	border:1px solid rgb(215,215,215);
	background:rgb(243,243,243);
}
.center_ul li{
	list-style:none;
	line-height:30px;
	height:30px;
	padding-left:20px;

}
/*分类*/
.center_classify{
	margin-top:15px; 
	padding-left:10px;
	font-size:15px; 
}
/*右边部分*/
.center_right{
	width:830px;	
	height:635px;
	float:left;
	margin-left:10px; 
}
/*************个人中心首页样式********************/
.index_indent{
	color:rgb(49,101,176);
	float:right;
}
.index_h1{
	float:left;
	font-size:15px;
}
.index_title{
	height:40px;
	line-height:40px;
}
.index_details{
	height:30px;
	background:rgb(243,243,243);
	line-height:30px;
	
}
.index_details ul li{
	list-style:none;
	width:130px;
	float:left;
	text-align:center;
}
.index_content{
	height:30px;
	line-height:30px;
}
.index_content ul li{
	list-style:none;
	width:130px;
	float:left;
	text-align:center;
}
.indent_border{
	border:1px solid rgb(215,215,215);
	border-top:1px solid rgb(49,101,176);
}

/********我的订单页面样式**********/
.center_indent{
	height:30px;
	font-size:14px;
}
/*普通订单和充值订单一大块*/
.center_common{
	height:30px;
	border-bottom:1px solid rgb(49,101,176);
	margin-top:10px; 

}
.center_common ul li{
	width:75px;
	height:30px;
	line-height:30px;
	margin-left:10px;
	float:left;
	text-align:center;
	border:1px solid rgb(49,101,176); 
	border-bottom:0px; 
}
.center_state{
	height:30px;
	line-height:30px;
	float:right;
	color:rgb(49,101,176);
}
.center_qq{
	height:30px;
	line-height:30px;
	color:rgb(49,101,176);
}
/*订单详情*/
.center_details{
	height:40px;
	line-height:40px;
	border:1px solid #ccc;
}
.center_details ul li{
	list-style:none;
	float:left;
	width:170px;
	height:40px;
	line-height:40px;
	text-align:center;
}
/*********我的收藏页面样式**************/
.collect_goods{
	margin-top:10px; 
	height:30px;
	line-height:40px;
	background:#ccc;
}
.collect_goods ul li{
	list-style:none;
	float:left;
	width:170px;
	height:30px;
	line-height:30px;
	text-align:center;
}
.collect_striping{
	height:1px;
	margin-top:20px; 
	background:#ccc;
}
.collect_poster{
	margin-top:10px;
	height:126px;
}
/**********我的积分页面样式***************/
.scores_usable{
	height:30px;
}
.scores_usable ul li{
	width:100px;
	height:25px;
	line-height:25px;
	list-style:none;
	float:left;

}
/***************价格保护**********************/
.protect_details{
	height:30px;
	margin-top:10px; 
	background:#ccc;
}
.protect_details ul li{
	list-style:none;
	float:left;
	width:110px;
	height:30px;
	line-height:30px;
	text-align:center;
}
/*************个人资料管理****************/
.data_manage{
	height:30px;
}
.data_manage ul li{
	list-style:none;
	float:left;
	height:30px;
	line-height:30px;
}
.data_details{
	height:30px;
	line-height:30px;
	background:#ccc;
	margin-top:10px;
	padding-left:10px; 
}
.data_account{
	height:40px;
	line-height:40px;
	margin-top:20px;
}
.data_login{
	width:70px;
	height:40px;
	line-height:40px;
	float:left;
	padding-left:10px; 

}
.data_name{
	width:165px;
	height:35px;
	float:left;
}
.uname{
	width:165px;
	height:25px;
}
.data_sex{
	width:44px;
	height:40px;
	line-height:40px;
	float:left;
	padding-left:35px; 
}
.data_sex1{
